{
    "builds": [
        {
            "src": "project/wsgi.py",
            "use": "@vercel/python",
            "config": { "maxLambdaSize": "15mb", "runtime": "python3.12" }
          },
          {
            "src": "build.sh",
            "use": "@vercel/static-build"
          }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
          },
        {
        "src": "/(.*)",
        "dest": "project/wsgi.py"
        }
    ]

}